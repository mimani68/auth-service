{
	"verbose": true,
	"debug": false,
	"exec": "node -r ts-node/register --inspect=0.0.0.0:9222 ./src/index.ts",
	"ignore": [
		"mochawesome-report",
		"node_modules",
		"./test",
		"**/*.d.ts",
		"*.test.ts",
		"*.spec.ts",
		"fixtures/*",
		"test/**/*",
		"docs/*"
	],
	"watch": ["./src"],
	"ext": "ts",
	"inspect": true
}