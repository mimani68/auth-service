{
  "name": "authentication-service",
  "version": "1.0.3",
  "private": true,
  "author": {
    "name": "mahdi imani",
    "email": "imani.mahdi@gmail.com"
  },
  "scripts": {
    "start": "npm run dev:watch:inspect:ts3",
    "start:dev": "./node_modules/.bin/nodemon --exec 'npm run start:dev'",
    "clean": "rm -rf ./dist",
    "install": "npm install; npm prune; npm audit fix --force",
    "build": "npm audit fix --force && npm run clean && tsc",
    "lint": "./node_modules/.bin/tslint --fix",
    "debug": "DEBUG=1 npm run start",
    "serve": "npm run build && node ./dist/index.js --prod",
    "dev:inspect:js": "node --inspect=0.0.0.0:9222 ./dist/index.js --dev",
    "dev:inspect:tsnode": "node -r ts-node/register --inspect=0.0.0.0:9222 src/index.ts",
    "dev:watch:inspect:js": "./node_modules/.bin/nodemon -L --inspect=0.0.0.0:9222 index.js",
    "dev:watch:inspect:ts1": "./node_modules/.bin/nodemon",
    "dev:watch:inspect:ts2": "./node_modules/.bin/nodemon --exec 'npm run dev:inspect:tsnode'",
    "dev:watch:inspect:ts3": "./node_modules/.bin/nodemon --exec 'npm run build && npm run dev:inspect:js'"
  },
  "dependencies": {
    "amqplib": "^0.5.5",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.8.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.3.5",
    "cors": "^2.8.4",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "finalhandler": "^1.1.1",
    "helmet": "^3.13.0",
    "jalali-moment": "^3.2.3",
    "jwt-simple": "^0.5.5",
    "mongoose": "^5.3.2",
    "morgan": "^1.9.1",
    "node-forge": "^0.7.6",
    "passport": "^0.4.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.13.2",
    "ts-node": "^8.3.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.11",
    "@types/helmet": "0.0.42",
    "@types/mongoose": "^5.2.18",
    "@types/morgan": "^1.7.35",
    "@types/node-forge": "^0.7.11",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/spdy": "^3.4.4",
    "nodemon": "^1.19.1",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.1.1"
  }
}
